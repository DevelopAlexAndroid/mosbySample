<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mosbysample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mosbyMVI.Activity"/>
        <activity android:name=".mosbyMVPViewState.ActivityViewState" />
        <activity android:name=".mosbyMVPLCE.ActivityLCE" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mosbyMVP.Activity">
            <intent-filter>
                <action android:name="android.intent.action.MosbyMVP" />
            </intent-filter>
        </activity>
    </application>

</manifest>